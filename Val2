handlerBackUrl = ($_SERVER['SERVER_PORT'] == 443 ? 'https' : 'http') . '://' . $_SERVER['SERVER_NAME'] . (in_array($_SERVER['SERVER_PORT'],
          array(80, 443)) ? '' : ':' . $_SERVER['SERVER_PORT']) . $_SERVER['SCRIPT_NAME'];
      if (data.action === 'close') {
        fetch(handlerBackUrl, {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
          },
          body: JSON.stringify({
            action: 'survey_results',
            surveyData: surveyData
            userId: <?php echo json_encode($_GET['USER_ID']); ?>,
            dialogId: <?php echo json_encode($_GET['DIALOG_ID']); ?>
          }),
        });
      }
